<?xml version="1.0" encoding="UTF-8" ?>
<Shell
    x:Class="Game_Esports.AppShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:local="clr-namespace:Game_Esports"
    Shell.FlyoutBehavior="Flyout">

    <ShellContent
        Title="Player Input"
        ContentTemplate="{DataTemplate local:MainPage}"
        Route="MainPage" />
    <FlyoutItem Title="Player Info">
        <ShellContent ContentTemplate="{DataTemplate local:PlayerInfo}">

        </ShellContent>
    </FlyoutItem>


</Shell>
